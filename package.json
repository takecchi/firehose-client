{
  "name": "@takecchi/firehose-client",
  "version": "0.0.2",
  "license": "MIT",
  "description": "Client for ATProtocol Firehose",
  "keywords": [
    "atproto",
    "bluesky",
    "api"
  ],
  "homepage": "https://github.com/takecchi/firehose-client#readme",
  "bugs": {
    "url": "https://github.com/takecchi/firehose-client/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/takecchi/firehose-client.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc --build tsconfig.build.json",
    "format": "prettier --write \"**/*.{js,ts}\"",
    "prepare": "husky install",
    "lint": "eslint \"{src,test}/**/*.{js,ts}\" --fix",
    "lint-staged": "lint-staged",
    "prepublishOnly": "tsc --build tsconfig.build.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@types/ws": "8.5.10",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "eslint": "8.57.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "ts-jest": "29.1.2",
    "typescript": "5.4.3"
  },
  "dependencies": {
    "@atproto/api": "0.12.2",
    "@atproto/common": "0.4.0",
    "@atproto/identity": "0.4.0",
    "@ipld/car": "3.2.4",
    "ws": "8.16.0"
  }
}
